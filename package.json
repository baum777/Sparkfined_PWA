{
  "name": "extracted",
  "private": true,
  "version": "0.1.0-alpha",
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b tsconfig.build.json && vite build",
    "build:local": "pnpm build && pnpm check:size",
    "build:ci": "pnpm build:local && pnpm test:e2e",
    "preview": "vite preview",
    "analyze": "cross-env ANALYZE=true vite build",
    "lighthouse": "npx lighthouse http://localhost:4173 --view --only-categories=performance,pwa,best-practices,accessibility",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier . --write",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "check:size": "node scripts/check-bundle-size.mjs"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.0",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0",
    "@vercel/node": "^3.0.0",
    "dexie": "^3.2.0",
    "driver.js": "^1.3.6",
    "lucide-react": "^0.552.0",
    "openai": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "tesseract.js": "^5.0.0",
    "vite-plugin-pwa": "^0.20.0",
    "web-push": "^3.6.7",
    "workbox-window": "^7.1.0",
    "ws": "^8.18.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.35.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.21",
    "vitest": "^1.6.0"
  }
}
